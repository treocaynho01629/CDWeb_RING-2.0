{
  "name": "@ring/redux",
  "version": "0.0.0",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./store": {
      "import": "./dist/src/lib/store.js",
      "require": "./dist/src/lib/store.cjs"
    },
    "./hooks": {
      "import": "./dist/src/lib/hooks.js",
      "require": "./dist/src/lib/hooks.cjs"
    },
    "./apiSlice": {
      "import": "./dist/src/lib/apiSlice.js",
      "require": "./dist/src/lib/apiSlice.cjs"
    },
    "./authReducer": {
      "import": "./dist/src/features/auth/authReducer.js",
      "require": "./dist/src/features/auth/authReducer.cjs"
    },
    "./authApiSlice": {
      "import": "./dist/src/features/auth/authApiSlice.js",
      "require": "./dist/src/features/auth/authApiSlice.cjs"
    },
    "./enumReducer": {
      "import": "./dist/src/features/enums/enumReducer.js",
      "require": "./dist/src/features/enums/enumReducer.cjs"
    },
    "./enumsApiSlice": {
      "import": "./dist/src/features/enums/enumsApiSlice.js",
      "require": "./dist/src/features/enums/enumsApiSlice.cjs"
    },
    "./booksApiSlice": {
      "import": "./dist/src/features/books/booksApiSlice.js",
      "require": "./dist/src/features/books/booksApiSlice.cjs"
    },
    "./bannersApiSlice": {
      "import": "./dist/src/features/banners/bannersApiSlice.js",
      "require": "./dist/src/features/banners/bannersApiSlice.cjs"
    },
    "./categoriesApiSlice": {
      "import": "./dist/src/features/categories/categoriesApiSlice.js",
      "require": "./dist/src/features/categories/categoriesApiSlice.cjs"
    },
    "./couponsApiSlice": {
      "import": "./dist/src/features/coupons/couponsApiSlice.js",
      "require": "./dist/src/features/coupons/couponsApiSlice.cjs"
    },
    "./publishersApiSlice": {
      "import": "./dist/src/features/publishers/publishersApiSlice.js",
      "require": "./dist/src/features/publishers/publishersApiSlice.cjs"
    },
    "./provider": {
      "import": "./dist/src/app/provider.js",
      "require": "./dist/src/app/provider.cjs"
    },
    "./storeRef": {
      "import": "./dist/src/app/storeRef.js",
      "require": "./dist/src/app/storeRef.cjs"
    },
    "./useEnums": {
      "import": "./dist/src/hooks/useEnums.js",
      "require": "./dist/src/hooks/useEnums.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\"",
    "update-dependencies": "npx npm-check-updates -u"
  },
  "devDependencies": {
    "@ring/eslint-config": "*",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-redux": "^7.1.34",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "lodash-es": "^4.17.21",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0"
  }
}
